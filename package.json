{
  "name": "skeban",
  "productName": "Skeban",
  "version": "0.3.3",
  "description": "Sketches your brain in kanban",
  "main": "./build/index.js",
  "repository": "https://github.com/roottool/skeban.git",
  "author": "roottool",
  "license": "MIT",
  "scripts": {
    "build:dev:main": "webpack --config ./config/webpack.dev.config.main.ts",
    "build:dev:renderer": "webpack --config ./config/webpack.dev.config.renderer.ts",
    "build:prod:main": "webpack --config ./config/webpack.prod.config.main.ts",
    "build:prod:renderer": "webpack --config ./config/webpack.prod.config.renderer.ts",
    "build": "rimraf build && concurrently \"yarn run build:prod:main\" \"yarn run build:prod:renderer\"",
    "build:dev": "rimraf build && concurrently \"yarn run build:dev:main\" \"yarn run build:dev:renderer\"",
    "build:watch": "rimraf build && concurrently \"yarn run build:dev:main\" \"webpack --config ./config/webpack.dev.config.renderer.ts --watch\"",
    "start": "electron .",
    "fix": "eslint --fix \"config/**\" && eslint --fix \"src/**\"",
    "lint:script": "eslint \"config/**\" && eslint \"src/**\"",
    "lint:style": "stylelint \"src/**\"",
    "lint": "yarn run lint:script && yarn run lint:style",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "jp.roottool.skeban",
    "productName": "Skeban",
    "copyright": "Copyright Â© 2019 roottool",
    "asar": true,
    "npmRebuild": false,
    "files": [
      "build/**/*"
    ],
    "mac": {
      "category": "public.app-category.developer-tools",
      "target": "dmg",
      "icon": "build/icons/icon.png"
    },
    "win": {
      "target": "nsis",
      "icon": "build/icons/icon.ico"
    },
    "publish": [
      {
        "provider": "github",
        "releaseType": "release"
      }
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.12.1",
    "@babel/plugin-proposal-optional-chaining": "^7.12.7",
    "@babel/preset-env": "^7.12.10",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@babel/register": "^7.12.10",
    "@types/copy-webpack-plugin": "^6.3.0",
    "@types/draft-js": "^0.10.44",
    "@types/html-webpack-plugin": "^3.2.4",
    "@types/markdown-to-jsx": "^6.11.3",
    "@types/prismjs": "^1.16.2",
    "@types/react": "^17.0.0",
    "@types/react-beautiful-dnd": "^13.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.6",
    "@types/rehype-react": "^4.0.0",
    "@types/showdown": "^1.9.3",
    "@types/styled-components": "^5.1.5",
    "@types/uuid": "^8.3.0",
    "@types/webpack": "^4.41.25",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "babel-loader": "^8.2.2",
    "concurrently": "^5.3.0",
    "copy-webpack-plugin": "^7.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.1",
    "electron": "^11.1.0",
    "electron-builder": "^22.9.1",
    "electron-store": "^6.0.1",
    "eslint": "^7.15.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.0.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.2.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.5.0",
    "monaco-editor": "^0.21.2",
    "monaco-editor-webpack-plugin": "^2.0.0",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.0",
    "style-loader": "^2.0.0",
    "stylelint": "^13.8.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recess-order": "^2.3.0",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-custom-processor-loader": "^0.6.0",
    "stylelint-prettier": "^1.1.1",
    "stylelint-processor-styled-components": "^1.9.0",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.8.0",
    "typescript": "^4.1.2",
    "typescript-styled-plugin": "^0.15.0",
    "webpack": "^5.10.0",
    "webpack-cli": "^4.2.0"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "codemirror": "^5.58.3",
    "dexie": "^3.0.3",
    "highlight.js": "^10.4.1",
    "moment": "^2.29.1",
    "react": "^17.0.1",
    "react-beautiful-dnd": "^13.0.0",
    "react-dom": "^17.0.1",
    "react-monaco-editor": "^0.40.0",
    "react-router-dom": "^5.0.1",
    "rehype-highlight": "^4.0.0",
    "rehype-react": "^6.1.0",
    "remark-parse": "^9.0.0",
    "remark-rehype": "^8.0.0",
    "styled-components": "^5.2.1",
    "typeface-roboto": "^1.1.13",
    "unified": "^9.2.0",
    "unstated-next": "^1.1.0"
  }
}
